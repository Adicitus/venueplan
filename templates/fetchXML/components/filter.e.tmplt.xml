<filter <<
	
	Render-Template "$TemplateDir\attributes.a.tmplt.xml" @{
		AcceptedAttributes = @( "type" )
		Defaults = @{ type = "and" }
		Entity = $Values
	}
>> >

<<
	$Filter.Conditions | % { $_ } | % {
		Render-Template "$TemplateDir\condition.e.tmplt.xml" $_
	}
>>

<<
	$Filter.Filters | ? { $_ } | % {
		Render-Template "$TemplateDir\components\filter.e.tmplt.xml" @{
			Filter = $_
		}
	}
>>

</filter>